{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "logoFile": "swansea-university-logo-white.png",
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Smart City Vis",
    "footer": "",
    "copyright": "Â© 2017 - Smart City Vis - Qiru Wang",
    "navType": "vertical",
    "theme": "yeti",
    "linenums": true,
    "collapseSymbols": false,
    "protocol": "html://",
    "methodHeadingReturns": true
  },
  "recurseDepth": 10,
  "source": {
    "include": ["../js/"],
    "exclude": ["../js/lib/"],
    "includePattern": ".+\\.js$"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}